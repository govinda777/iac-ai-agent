# Configuração de Ambiente para Testes BDD
# Copie este arquivo para .env.test e configure as variáveis

# =============================================================================
# MODO DE TESTE
# =============================================================================
# Valores possíveis: mock, integration, real, hybrid
TEST_MODE=mock

# =============================================================================
# CONFIGURAÇÕES PARA TESTES COM MOCKS
# =============================================================================
# Usuário mockado para testes
TEST_MOCK_USER_ID=mock_user_123

# Wallet mockada para testes
TEST_MOCK_WALLET_ADDRESS=0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb

# Configurações de comportamento dos mocks
TEST_MOCK_FAILURES=false
TEST_MOCK_RESPONSE_TIME=100ms
TEST_MOCK_ERROR_RATE=0.1
TEST_MOCK_DATA_CONSISTENCY=true

# =============================================================================
# CONFIGURAÇÕES PARA TESTES DE INTEGRAÇÃO REAL
# =============================================================================
# Privy.io - App ID padrão para testes
TEST_REAL_PRIVY_APP_ID=cmgh6un8w007bl10ci0tgitwp

# Base Network - RPC URL para testes
TEST_REAL_BASE_RPC_URL=https://goerli.base.org

# Contratos deployados na Base Network
TEST_REAL_NFT_CONTRACT_ADDR=0x147e832418Cc06A501047019E956714271098b89
TEST_REAL_TOKEN_CONTRACT_ADDR=0x8f3Cf7ad23Cd3CaDbD9735AFF958023239c6A063

# Nation.fun - API Key para análise de código
TEST_REAL_NATION_API_KEY=your_nation_api_key_here

# OpenAI - API Key para LLM
TEST_REAL_OPENAI_API_KEY=your_openai_api_key_here

# Anthropic - API Key para Claude
TEST_REAL_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# =============================================================================
# CONFIGURAÇÕES DE PERFORMANCE E CARGA
# =============================================================================
# Habilitar testes de performance
TEST_PERFORMANCE_ENABLED=false

# Número de usuários para testes de carga
TEST_LOAD_COUNT=10

# Timeout para operações (em segundos)
TEST_TIMEOUT_SECONDS=30

# Configurações de rate limiting
TEST_RATE_LIMIT_REQUESTS_PER_MINUTE=60
TEST_RATE_LIMIT_REQUESTS_PER_HOUR=1000

# =============================================================================
# CONFIGURAÇÕES DE LOG E DEBUG
# =============================================================================
# Nível de log (debug, info, warn, error)
TEST_LOG_LEVEL=info

# Formato de log (json, text)
TEST_LOG_FORMAT=json

# Habilitar logs de debug
TEST_DEBUG=false

# Habilitar logs de performance
TEST_PERFORMANCE_DEBUG=false

# =============================================================================
# CONFIGURAÇÕES DE BANCO DE DADOS
# =============================================================================
# URL do banco de dados para testes
TEST_DATABASE_URL=postgres://user:password@localhost:5432/iac_ai_agent_test

# Configurações de conexão
TEST_DB_MAX_CONNECTIONS=10
TEST_DB_CONNECTION_TIMEOUT=30s

# =============================================================================
# CONFIGURAÇÕES DE CACHE
# =============================================================================
# URL do Redis para testes
TEST_REDIS_URL=redis://localhost:6379/1

# Configurações de cache
TEST_CACHE_TTL=300s
TEST_CACHE_MAX_SIZE=100MB

# =============================================================================
# CONFIGURAÇÕES DE MONITORAMENTO
# =============================================================================
# Habilitar métricas de teste
TEST_METRICS_ENABLED=true

# Endpoint para métricas
TEST_METRICS_ENDPOINT=http://localhost:9090/metrics

# =============================================================================
# CONFIGURAÇÕES DE SEGURANÇA
# =============================================================================
# Chave secreta para testes
TEST_SECRET_KEY=test_secret_key_123

# Configurações de JWT
TEST_JWT_SECRET=test_jwt_secret_456
TEST_JWT_EXPIRY=24h

# =============================================================================
# CONFIGURAÇÕES DE NOTIFICAÇÕES
# =============================================================================
# Webhook para notificações de teste
TEST_WEBHOOK_URL=https://hooks.slack.com/services/TEST/WEBHOOK/URL

# Email para notificações
TEST_NOTIFICATION_EMAIL=test@example.com

# =============================================================================
# CONFIGURAÇÕES DE ARQUIVOS E STORAGE
# =============================================================================
# Diretório para arquivos temporários de teste
TEST_TEMP_DIR=/tmp/iac_ai_agent_test

# Configurações de S3 para testes
TEST_S3_BUCKET=iac-ai-agent-test
TEST_S3_REGION=us-east-1

# =============================================================================
# CONFIGURAÇÕES DE CI/CD
# =============================================================================
# Executar em ambiente CI/CD
TEST_CI_ENVIRONMENT=false

# Configurações específicas para CI
TEST_CI_PARALLEL_WORKERS=4
TEST_CI_TIMEOUT=10m

# =============================================================================
# CONFIGURAÇÕES DE COBERTURA DE CÓDIGO
# =============================================================================
# Habilitar cobertura de código
TEST_COVERAGE_ENABLED=false

# Diretório para relatórios de cobertura
TEST_COVERAGE_DIR=./coverage

# Threshold mínimo de cobertura
TEST_COVERAGE_THRESHOLD=80

# =============================================================================
# CONFIGURAÇÕES DE RELATÓRIOS
# =============================================================================
# Gerar relatórios HTML
TEST_HTML_REPORTS=true

# Gerar relatórios JSON
TEST_JSON_REPORTS=true

# Diretório para relatórios
TEST_REPORTS_DIR=./reports

# =============================================================================
# CONFIGURAÇÕES DE RETRY E RESILIENCE
# =============================================================================
# Número máximo de tentativas
TEST_MAX_RETRIES=3

# Intervalo entre tentativas
TEST_RETRY_INTERVAL=1s

# Backoff exponencial
TEST_EXPONENTIAL_BACKOFF=true

# =============================================================================
# CONFIGURAÇÕES DE VALIDAÇÃO
# =============================================================================
# Validar configurações na inicialização
TEST_VALIDATE_CONFIG=true

# Validar conectividade com serviços externos
TEST_VALIDATE_EXTERNAL_SERVICES=false

# =============================================================================
# CONFIGURAÇÕES DE LIMPEZA
# =============================================================================
# Limpar dados de teste após execução
TEST_CLEANUP_AFTER_TESTS=true

# Limpar arquivos temporários
TEST_CLEANUP_TEMP_FILES=true

# Limpar dados do banco
TEST_CLEANUP_DATABASE=true

# =============================================================================
# CONFIGURAÇÕES DE PARALELIZAÇÃO
# =============================================================================
# Executar testes em paralelo
TEST_PARALLEL_EXECUTION=false

# Número de workers paralelos
TEST_PARALLEL_WORKERS=4

# =============================================================================
# CONFIGURAÇÕES DE FILTROS
# =============================================================================
# Executar apenas testes críticos
TEST_CRITICAL_ONLY=false

# Executar apenas testes rápidos
TEST_FAST_ONLY=false

# Executar apenas testes lentos
TEST_SLOW_ONLY=false

# =============================================================================
# CONFIGURAÇÕES DE NOTIFICAÇÕES DE ERRO
# =============================================================================
# Enviar notificações em caso de falha
TEST_ERROR_NOTIFICATIONS=false

# Canal Slack para notificações de erro
TEST_ERROR_SLACK_CHANNEL=#test-failures

# =============================================================================
# CONFIGURAÇÕES DE BACKUP E RESTORE
# =============================================================================
# Fazer backup antes dos testes
TEST_BACKUP_BEFORE_TESTS=false

# Restaurar backup após os testes
TEST_RESTORE_AFTER_TESTS=false

# Diretório de backup
TEST_BACKUP_DIR=./backups